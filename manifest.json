{
    "name": "TwiShell",
    "version": "3.13",
    "manifest_version": 2,
    "description": "Enhance Twitter Web with lots of features.",
    "icons": {
        "16": "images/16.ico",
        "48": "images/48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "TwiShell.user.js"
            ],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "*://twitter.com/*"
    ]
}
