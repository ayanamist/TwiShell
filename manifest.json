{
    "name": "TwiShell",
    "version": "1.1",
    "manifest_version": 2,
    "description": "Enhance Twitter Web with lots of features.",
    "icons": {
        "16": "favicon.ico",
        "48": "twitter-bird-light-bgs.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "css": [
                "Twitter Left Timeline.css"
            ],
            "js": [
                "Twitter Tweet Hotkey.user.js",
                "Twitter Traditional RT Style.user.js",
                "Twitter Expand URL.js",
                "Instagram preview in Twitter.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "*://twitter.com/*"
    ]
}